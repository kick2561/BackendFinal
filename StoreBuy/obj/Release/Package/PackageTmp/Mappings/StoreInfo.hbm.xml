<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
   xmlns="urn:nhibernate-mapping-2.2"
   auto-import ="true"
   assembly="StoreBuy"
   namespace="StoreBuy.Domain">


  <class name="StoreInfo" table="StoreInfo" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="StoreId" column="StoreId" type="long">
      <generator class="native" />
    </id>
    <property name="StoreName"  />
    <property name="Phone"  />
    <property name="SlotDays"/>
    <property name="SlotStartTime"/>
    <property name="SlotEndTime"/>
    <property name="SlotDuration"/>
    <property name="Email"/>

    <many-to-one
        name="StoreLocation" column="StoreLocationId" />
  </class>
  
</hibernate-mapping>